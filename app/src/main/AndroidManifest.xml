    <application
        android:name="com.janlent.sodexo.app.App"
        android:configChanges="orientation|keyboardHidden"
        android:icon="@drawable/app_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:theme="@style/ThemeActivity" >
        <activity
            android:name="com.janlent.sodexo.activity.LoginActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.janlent.sodexo.activity.OrderActivity"
            android:configChanges="orientation|keyboardHidden"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.janlent.sodexo.activity.FoodDetailActivity"
            android:configChanges="orientation|keyboardHidden"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.janlent.sodexo.activity.ConfirmOrderActivity"
            android:configChanges="orientation|keyboardHidden"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.janlent.sodexo.activity.PatientInfoActivity"
            android:configChanges="orientation|keyboardHidden"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateHidden|adjustPan" />
        <activity
            android:name="com.janlent.sodexo.activity.PatientsListActivity"
            android:configChanges="orientation|keyboardHidden"
            android:launchMode="singleTask"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.janlent.sodexo.activity.SynchronousDataActivity"
            android:configChanges="orientation|keyboardHidden"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.janlent.sodexo.activity.CameraActivity"
            android:configChanges="orientation|keyboardHidden"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.janlent.sodexo.activity.ImgGridActivity"
            android:configChanges="orientation|keyboardHidden"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.janlent.sodexo.activity.SettingActivity"
            android:configChanges="orientation|keyboardHidden"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.janlent.sodexo.activity.DeviceActivity"
            android:configChanges="orientation|keyboardHidden"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustUnspecified|stateHidden" />
        <activity
            android:name="com.janlent.sodexo.activity.ShowDatabaseActivity"
            android:configChanges="orientation|keyboardHidden"
            android:screenOrientation="portrait" />
        <activity android:name="com.janlent.sodexo.activity.ImageListActivity" >
        </activity>
        <activity android:name="com.janlent.sodexo.activity.Image3DActivity" >
        </activity>
        <activity android:name="com.janlent.sodexo.activity.ImageActivtiy" >
        </activity>

        <service android:name="com.janlent.utils.LogService" >
        </service>
